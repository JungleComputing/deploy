#!/bin/sh

#defaults to current directory, set in environment to override if necessary 
if [ -z "$DEPLOY_HOME" ];  then
	DEPLOY_HOME=.
fi

MAIN_CLASS=amuseVisualization.AmuseVisualization

exec java -Xmx500m -Dapple.laf.useScreenMenuBar=true -classpath $DEPLOY_HOME:$DEPLOY_HOME/lib/* -Djava.library.path=$DEPLOY_HOME/lib/natives -Dibis.deploy.home=$DEPLOY_HOME $MAIN_CLASS $*
